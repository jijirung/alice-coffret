        }

        .ProjectIntroduction__StatusValue {
            font-size: 20pt;
            font-weight: 800;
        }

        .ProjectIntroduction__Small {
            font-size: 8pt;
            font-weight: 400;
        }

        .ProjectIntroduction__FundingRate {
            font-size: 10pt;
            font-weight: 800;
        }

        .FundingInformation__FundingInformationInner {
            padding: 20px;
            margin-top: 10px;
            background: #fafafa;
            border: 0.5px solid #EAEAEA;
            border-radius: 5px;
        }

        .FundingInformation__FundingInformationInner > div {
