        .ProjectIntroduction_ProjectOutline {
            width: 900px;
            margin: 0 auto;
            text-align: center;
        }

        .ProjectIntroduction__ProjectCategory {
            color: #757575;
            font-size: 10px;
            background: #fafafa;
            padding: 4px;
            border: 1px solid #EAEAEA;
            border-radius: 3px;
