        }

        .ProjectIntroduction__CreatorName {
            color: black;
            text-decoration: none;
        }

        .ProjectIntroduction__ProjectIntroductionMain {
            width: 950px;
            height: 400px;
            margin: 0 auto;
        }

        .ProjectIntroduction__ProjectCover {
            height: 400px;
        }

        .ProjectIntroduction__ProjectIntroductionMainColumn {
            float: left;
        }

        .ProjectIntroduction__FundingStatuss {
