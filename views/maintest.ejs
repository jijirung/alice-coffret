    <div class="content">
        <h2>이달의 상품</h2>
        <section>
            <div class="wrapper">
                <div class="banner"></div>
                <div class="bxslider">
                    <% for(var i=0; i< session.bannerImg.list.length; i++){ 
                        imgList=session.bannerImg.list[i]
                    %>
                    <div>
                        <h3><%= imgList.product_name %></h3>
                        <div class="tag_box">
                            <% for(var j=0; j< imgList.tag.length; j++){
                                var tag=imgList.tag[j]
                                var tag_code
                                if(tag=="봄"){
                                    tag_code="tag_spring"
                                }else if(tag=="여름"){
                                    tag_code="tag_summer"
                                }else if(tag=="가을"){
                                    tag_code="tag_fall"
                                }else if(tag=="겨울"){
                                    tag_code="tag_winter"
                                }else if(tag=="큐트"){
                                    tag_code="tag_cute"
                                }else if(tag=="화려"){
                                    tag_code="tag_brilliant"
                                }else if(tag=="우아"){
                                    tag_code="tag_luxury"
                                }else if(tag=="심플"){
                                    tag_code="tag_simple"
                                } %>
                            <span class=<%= tag_code %>><%= imgList.tag[j] %></span>
                            <% } %>
                        </div>
                        <img alt="" style="width: 100%;" src=<%=imgList.thumbnail %> />
                    </div>
                    <% } %>
                </div>
            </div>
    </div>
    </section>
